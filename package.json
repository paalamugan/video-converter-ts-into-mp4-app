{
  "name": "video-converter-ts-into-mp4-app",
  "description": "Convert media .ts files into .mp4 videos and download them using remix framework",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "yarn lint --fix && yarn format",
    "typecheck": "tsc",
    "type-check": "tsc --noEmit",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@remix-run/node": "^1.14.1",
    "@remix-run/react": "^1.14.1",
    "@remix-run/vercel": "^1.14.1",
    "@vercel/node": "^2.6.2",
    "@vercel/remix-entry-server": "^0.1.0",
    "framer-motion": "^6",
    "fs-extra": "^11.1.0",
    "isbot": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "video-converter-ts-into-mp4": "^0.2.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@remix-run/dev": "^1.14.1",
    "@remix-run/eslint-config": "^1.14.1",
    "@remix-run/serve": "^1.14.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "commitizen": "^4.3.0",
    "commitlint": "^17.4.4",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "^4.8.4"
  },
  "license": "MIT",
  "packageManager": "yarn@1.22.19",
  "author": "paalamugan",
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22",
    "npm": "use yarn"
  },
  "keywords": [
    "remix",
    "react",
    "typescript",
    "vite",
    "vitest",
    "cypress",
    "msw",
    "prisma",
    "chakra-ui",
    "video-converter-ts-into-mp4"
  ]
}